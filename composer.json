{
    "name": "bsiorg/primetrust-client-laravel",
    "description": "A api client implementation in laravel of primetrust api.",
    "tags": [
        "bsiorg",
        "api",
        "package",
        "primetrust",
        "laravel",
        "primetrust-client-laravel"
    ],
    "homepage": "https://github.com/bsiorg/primetrust-client-laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mubashir Rasool Razvi",
            "email": "rizimore@outlook.com",
            "role": "Director | Team Lead | Senior Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "BsiOrg\\PrimeTrust\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BsiOrg\\PrimeTrust\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.2.5|^8.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench": "5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BsiOrg\\PrimeTrust\\PrimeTrustServiceProvider"
            ],
            "aliases": {
                "PrimeTrust": "BsiOrg\\PrimeTrust\\PrimeTrustFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
